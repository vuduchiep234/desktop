<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"classpath://nttt/api/configdb/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="nttt.api.model.TaiKhoanKhaiThacDichVu" table="TaiKhoanKhaiThacDichVu" dynamic-update="true">
        
        <meta attribute = "class-description">
            This class contains the sesion detail. 
        </meta>  
      
        <id name="id" type = "int" column = "id">
            <generator class="sequence">
                <param name="sequence">tai_khoan_khai_thac_dich_vu_id_sequence</param>
            </generator>
        </id>
        <property name="serviceId" type="java.lang.Integer">
            <column name="serviceId"/>
        </property> 
        <property name="partnerId" type="java.lang.Integer">
            <column name="partnerId"/>
        </property> 
    </class>
</hibernate-mapping>