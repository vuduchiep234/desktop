<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"classpath://nttt/api/configdb/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="nttt.api.model.CongThanhToan" table="CongThanhToan" dynamic-update="true">
    
        <meta attribute = "class-description">
            This class contains the sesion detail. 
        </meta>  
      
        <id name="id" type = "int" column = "id">
            <generator class="sequence">
                <param name="sequence">cong_thanh_toan_id_sequence</param>
            </generator>
        </id>
        <property name="name" type="java.lang.String">
            <column name="name"/>
        </property>
    </class>
</hibernate-mapping>